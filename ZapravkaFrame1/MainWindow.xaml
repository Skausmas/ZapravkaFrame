<Window x:Class="ZapravkaFrame1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZapravkaFrame1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="#FFD0B1B1">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="103*"></ColumnDefinition>
            <ColumnDefinition Width="297*">
            </ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Frame x:Name="Frame1" Grid.Column="1" Grid.RowSpan="3" Navigated="Frame_Navigated" NavigationUIVisibility="Hidden"/>
        
        <Button Content="Gas Station" HorizontalAlignment="Center" VerticalAlignment="Center" Height="217" Width="206" Background="#FF78A6D0" Click="Button_Click"/>
        <Button Content="Cafe" HorizontalAlignment="Center" Height="218" Grid.Row="1" VerticalAlignment="Center" Width="206" Background="#FFE97FE0" Click="Button_Click_1"/>

    </Grid>

</Window>
